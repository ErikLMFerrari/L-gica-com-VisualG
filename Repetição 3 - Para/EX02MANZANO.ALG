Algoritmo "Ex02Manzano"

Var
   N1, Cont, Result: inteiro


Inicio
      Escreval("Escreva um número")
      Leia(N1)
      LimpaTela
      
      Para Cont <- 1 ate 10 faca
      
      Result <- N1 * Cont
      Escreval(N1 ," * ", Cont, " = ", Result)
      
      FimPara


Fimalgoritmo