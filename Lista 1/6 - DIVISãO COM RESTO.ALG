Algoritmo "Divisão"

Var
   N1, N2: inteiro
   Divi, Rest: Real


Inicio
      Escreval("Escreva o Dividendo")
      Leia(N1)
      Escreval("Escreva o Divisor")
      Leia(N2)
      Divi <-  N1 \ N2
      Rest <-  N1 % N2
      Escreval("A divisão possui total de:", Divi , " e o resto de", Rest)

Fimalgoritmo
