Algoritmo "Ex01Manzano"

Var
   N1, Cont, ToT: inteiro


Inicio
       Escreval("Digite um número")
       Leia(N1)
       LimpaTela
       Cont <- 1
       Enquanto (Cont <= 10) faca
            ToT <- N1 * Cont
            Escreval(N1," *", Cont, " =", ToT)
            Cont <- Cont + 1
       FimEnquanto



Fimalgoritmo