Algoritmo "CaixaRetangular"

Var
    H, C, L, V: real

Inicio
      Escreval("Descreva o comprimento da caixa")
      Leia(C)
      Escreval("Agora descreva a altura")
      Leia(H)
      Escreval("E por último a largura")
      Leia(L)
      
      V <- C*H*L
      
      Escreval("O volume da caixa é: ", V)


Fimalgoritmo